<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    >
    <Transition
        app:constraintSetEnd="@id/end"
        app:constraintSetStart="@+id/start"
        app:duration="1000"
        app:autoTransition="animateToEnd"
        >



    </Transition>
    <ConstraintSet android:id="@+id/start">

        <Constraint
            app:layout_editor_absoluteY="16dp"
            android:layout_height="wrap_content"
            app:layout_editor_absoluteX="161dp"
            android:layout_width="wrap_content"
            android:id="@+id/button" />
        <Constraint
            app:layout_editor_absoluteY="631dp"
            android:layout_height="wrap_content"
            app:layout_editor_absoluteX="337dp"
            android:layout_width="wrap_content"
            android:id="@+id/textView4" />
        <Constraint
            app:layout_editor_absoluteY="650dp"
            android:layout_height="wrap_content"
            app:layout_editor_absoluteX="37dp"
            android:layout_width="wrap_content"
            android:id="@+id/textView2" />
        <Constraint
            app:layout_editor_absoluteY="623dp"
            android:layout_height="wrap_content"
            app:layout_editor_absoluteX="37dp"
            android:layout_width="wrap_content"
            android:id="@+id/textView" />
        <Constraint
            app:layout_editor_absoluteY="611dp"
            android:layout_height="wrap_content"
            app:layout_editor_absoluteX="337dp"
            android:layout_width="wrap_content"
            android:id="@+id/textView3" />

    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">


        <Constraint
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:id="@+id/button"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"
            app:layout_constraintHorizontal_bias="0.498"
            android:layout_marginStart="32dp"
            android:layout_marginEnd="32dp"
            android:layout_marginBottom="16dp" />
        <Constraint
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/textView"
            android:layout_marginTop="96dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="48dp"
            android:layout_marginStart="48dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/textView"
            android:id="@+id/textView2"
            android:layout_marginTop="72dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="48dp"
            android:layout_marginStart="48dp" />
        <Constraint
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/textView3"
            android:layout_marginTop="96dp"
            app:layout_constraintStart_toEndOf="@+id/textView"
            app:layout_constraintEnd_toEndOf="parent" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/textView3"
            android:id="@+id/textView4"
            android:layout_marginTop="76dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView2" />
    </ConstraintSet>


</MotionScene>
